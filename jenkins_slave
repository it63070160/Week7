# your Home work
pipeline {
    agent any

    parameters {
        string(name: 'NAME', defaultValue: "Veerawat", description: "Enter your name")
        string(name: 'Ages', defaultValue: "21", description: "Enter your age")
    }

    stages {
        
        stage('Welcome to slave'){
            steps{
                echo 'Start Program'
            }
        }
        stage('slave execute'){
            steps{
                python myscript.py ${params.NAME} --age ${params.Ages}
            }
        }
    }
}